[%	WRAPPER 'inc/page_wrapper.tt';
	title = self.display_name;
	self = results.function;
%]

<style>

	/* Hookups for seqData tab group and content box */
 	#tab1_seqData:checked ~ #tabContent1_seqData,
 	#tab2_seqData:checked ~ #tabContent2_seqData {
 		display: block;
  	}

	/* comment this out while working on this to see my dev notes... */
	.noteToSelf {
		display: none;
	}

</style>

[%
	self.db = 'CyCOG';
	self.xref = self.cycog_oid;
	PROCESS 'inc/title/function.tt';


%]

<div class="thisWrapper">
	<div id="pageHeader">
		<div id="pageTitle" >
			<h4 class="normal">
				CyCOG Function
			</h4>
			<h2>[% self.name %]</h2>
		</div>
		<div id="buttonBar">
			<button>Add to Cart</button>
<!-- 			<button>Export</button> 	** develop this later **			-->
		</div>
	</div>
<div class="row">
	<div class="infoBox">
		<div class="infoBoxTitle"><h4>CyCOG Details</h4></div>
		<table class="condensed">
			<tr>
				<td class="col1">CyCOG ID</td>
				<td>[% self.cycog_oid %]</td>
			</tr>
			<tr>
				<td>Name</td>
				<td>[% self.name %]</td>
			</tr>
			<tr>
				<td>CyCOG version</td>
				<td>version [% self.version %]</td>
			</tr>
			<tr>
				<td>Gene members</td>
				<td>[% self.n_members %]</td>
			</tr>
			<tr>
				<td>MIT ID</td>
				<td>[% self.mit_oid %]</td>
			</tr>
		</table>
	</div> <!-- infoBox -->
	<div class="infoBox">


	<div class="infoBoxTitle"><h4>CyCOG Consensus Sequence</h4></div>

		<div class="nav_tabs nav_tabs_minimal">
			<input type="radio" name="seqData" id="tab1_seqData" checked >
			<label for="tab1_seqData">DNA</label>

			<input type="radio" name="seqData" id="tab2_seqData">
			<label for="tab2_seqData">Amino Acid</label>


			<div id="tabContent1_seqData" class="tabContent">
				<!-- <h4> DNA </h4> -->
				<div class="textDisplay">
				<code> <span style="color: blue">>640083263 DNA polymerase III, beta subunit (EC 2.7.7.7) [Prochlorococcus marinus str. NATL1A: NC_008819] (+)strand</span>
